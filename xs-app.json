{
"authenticationMethod": "route", 
"sessionTimeout": 10,
"routes": [
  {
   "source": "^/auds/webapp/(.*)$",
   "target": "$1",
   "localDir": "auds/webapp/",
   "authenticationType": "xsuaa",
   "csrfProtection": false
  },
  {
   "source": "^/gcr/webapp/(.*)$",
   "target": "$1",
   "localDir": "gcr/webapp/",
   "authenticationType": "xsuaa",
   "csrfProtection": false
  },
  {
   "source": "^/(.*)$",
   "target": "$1",
   "destination": "srv-api",
   "authenticationType": "xsuaa",
   "csrfProtection": false
  }
 
],
"logout": {
  "logoutEndpoint": "/my/logout",
  "logoutPage": "https://www.google.com"
}
}